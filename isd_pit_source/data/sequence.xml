<?xml version="1.0"?>
<odoo>
    <data noupdate='1'>
         <record id="seq_field_service_pit" model="ir.sequence">
            <field name="name">Field Service</field>
            <field name="code">project.task.sequence</field>
            <field name="prefix">FS_PIT</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_task_no_pit" model="ir.sequence">
            <field name="name">Task Number</field>
            <field name="code">project.task.number</field>
            <field name="prefix">PIT/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>